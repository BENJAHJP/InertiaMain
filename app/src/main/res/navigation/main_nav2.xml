<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav2"
    app:startDestination="@id/exploreFragment2">

    <fragment
        android:id="@+id/exploreFragment2"
        android:name="com.example.inertia.Dashboard.explore.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" >
        <action
            android:id="@+id/action_exploreFragment2_to_imageUpload"
            app:destination="@id/imageUpload" />
    </fragment>
    <fragment
        android:id="@+id/buildFragment2"
        android:name="com.example.inertia.Dashboard.BuildFragment"
        android:label="fragment_build"
        tools:layout="@layout/fragment_build" />
    <fragment
        android:id="@+id/trainFragment2"
        android:name="com.example.inertia.Dashboard.TrainFragment"
        android:label="fragment_train"
        tools:layout="@layout/fragment_train" />
    <fragment
        android:id="@+id/imageUpload"
        android:name="com.example.inertia.Dashboard.explore.ImageUpload"
        android:label="fragment_image_upload"
        tools:layout="@layout/fragment_image_upload" >
        <action
            android:id="@+id/action_imageUpload_to_exploreFragment2"
            app:destination="@id/exploreFragment2" />
    </fragment>
</navigation>